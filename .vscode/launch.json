{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch KubeHound",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/kubehound",
            "args": ["-c", "${workspaceFolder}/configs/etc/kubehound.yaml"],
            "env": {
                "KH.storage.retry": "6"
            }     
        }
    ]
}